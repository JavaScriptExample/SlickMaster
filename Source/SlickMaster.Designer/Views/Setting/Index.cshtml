
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>设置页</title>
</head>
<body>
    <!-- Modal -->
    <div class="modal-body">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="myModalLabel">设置</h3>
        </div>
        <div class="modal-body">
            <div class="row span2">
                <label>表单绑定流程: </label>
                <select class="form-control input-lg m-bot15" id="ddlProcesses">
                    <option value="null">--流程列表--</option>
                </select>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">取消</button>
            <button class="btn btn-primary" onclick="setting.sure();">确定</button>
        </div>
    </div>

    <script type="text/javascript" src="~/ViewJS/setting.js"></script>
    <script type="text/javascript">
        $(function () {
            setting.loadProcess();
        })
    </script>

</body>
</html>
